<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.algebra.java2.milionare.milionare.ResultsScreenController">
   <children>
      <Label fx:id="endMessage" alignment="CENTER" layoutX="100.0" layoutY="14.0" prefHeight="30.0" prefWidth="400.0" text="Load moves from XML" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextArea layoutX="101.0" layoutY="128.0" prefHeight="400.0" prefWidth="400.0" fx:id="xmlTextArea" />
      <Button fx:id="writeXml" layoutX="140.0" layoutY="66.0" mnemonicParsing="false" onAction="#writeToXml" prefHeight="35.0" prefWidth="120.0" text="1. Write xml" />
      <Button fx:id="readXml" layoutX="340.0" layoutY="66.0" mnemonicParsing="false" onAction="#readFromXml" prefHeight="35.0" prefWidth="120.0" text="2. Read xml" />
      <Button fx:id="endGame" layoutX="240.0" layoutY="588.0" mnemonicParsing="false" onAction="#Exit" prefHeight="35.0" prefWidth="120.0" text="Exit" />
   </children>
</AnchorPane>
